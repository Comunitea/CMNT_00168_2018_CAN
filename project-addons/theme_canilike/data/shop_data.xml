<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <record id="demo_category_1" model="product.public.category">
            <field name="name">Phones</field>
            <field name="parent_id"/>
            <field name="slug">phones</field>
            <field name="sequence">10</field>
        </record>
        <record id="demo_category_2" model="product.public.category">
            <field name="name">Tablets</field>
            <field name="parent_id"/>
            <field name="slug">tablets</field>
            <field name="sequence">20</field>
        </record>
        <record id="demo_category_3" model="product.public.category">
            <field name="name">Watches</field>
            <field name="parent_id"/>
            <field name="slug">watches</field>
            <field name="sequence">30</field>
        </record>

        <record id="demo_product_1" model="product.template">
            <field name="name">Android Phone</field>
            <field name="public_categ_ids" eval="[(6,0,[ref('demo_category_1')])]"/>
            <field name="list_price">325</field>
            <field name="type">product</field>
            <field name="website_published">true</field>
            <field name="default_code">AND001</field>
            <field name="image" type="base64" file="theme_canilike/static/demo/android-1.jpg"/>
            <field name="description_short">New mobile phone with Android 9.0 system</field>
        </record>
        <record id="demo_product_1_image_2" model="product.image">
            <field name="name">Android Phone - image #2</field>
            <field name="product_tmpl_id" ref="demo_product_1"/>
            <field name="image" type="base64" file="theme_canilike/static/demo/android-2.jpg"/>
        </record>

        <record id="demo_product_2" model="product.template">
            <field name="name">Apple iPhone</field>
            <field name="public_categ_ids" eval="[(6,0,[ref('demo_category_1')])]"/>
            <field name="list_price">715</field>
            <field name="type">product</field>
            <field name="website_published">true</field>
            <field name="default_code">IPH001</field>
            <field name="image" type="base64" file="theme_canilike/static/demo/apple-1.jpg"/>
            <field name="description_short">New Apple Iphone with incredible double camera</field>
        </record>
        <record id="demo_product_2_image_2" model="product.image">
            <field name="name">Apple iPhone - image #2</field>
            <field name="product_tmpl_id" ref="demo_product_2"/>
            <field name="image" type="base64" file="theme_canilike/static/demo/apple-2.jpg"/>
        </record>
        <record id="demo_product_2_image_3" model="product.image">
            <field name="name">Apple iPhone - image #3</field>
            <field name="product_tmpl_id" ref="demo_product_2"/>
            <field name="image" type="base64" file="theme_canilike/static/demo/apple-3.jpg"/>
        </record>

        <record id="demo_product_3" model="product.template">
            <field name="name">Apple iPad, White</field>
            <field name="public_categ_ids" eval="[(6,0,[ref('demo_category_2')])]"/>
            <field name="list_price">575</field>
            <field name="type">product</field>
            <field name="website_published">true</field>
            <field name="default_code">IPD001</field>
            <field name="image" type="base64" file="theme_canilike/static/demo/ipad-1.jpg"/>
        </record>
        <record id="demo_product_3_image_2" model="product.image">
            <field name="name">Apple iPad, White - image #2</field>
            <field name="product_tmpl_id" ref="demo_product_3"/>
            <field name="image" type="base64" file="theme_canilike/static/demo/ipad-2.jpg"/>
        </record>

        <record id="demo_product_4" model="product.template">
            <field name="name">Apple iPad, Black</field>
            <field name="public_categ_ids" eval="[(6,0,[ref('demo_category_2')])]"/>
            <field name="list_price">565</field>
            <field name="type">product</field>
            <field name="website_published">true</field>
            <field name="default_code">IPD002</field>
            <field name="image" type="base64" file="theme_canilike/static/demo/ipad-3.jpg"/>
        </record>
        <record id="demo_product_4_image_2" model="product.image">
            <field name="name">Apple iPad, Black - image #2</field>
            <field name="product_tmpl_id" ref="demo_product_4"/>
            <field name="image" type="base64" file="theme_canilike/static/demo/ipad-4.jpg"/>
        </record>

        <record id="demo_product_5" model="product.template">
            <field name="name">Smart watch</field>
            <field name="public_categ_ids" eval="[(6,0,[ref('demo_category_3')])]"/>
            <field name="list_price">125</field>
            <field name="type">product</field>
            <field name="website_published">true</field>
            <field name="default_code">SWT001</field>
            <field name="image" type="base64" file="theme_canilike/static/demo/smart-1.jpg"/>
            <field name="description_short">New modern smart watch with Android and iOS compatibility</field>
        </record>
        <record id="demo_product_5_image_2" model="product.image">
            <field name="name">Smart watch - image #2</field>
            <field name="product_tmpl_id" ref="demo_product_5"/>
            <field name="image" type="base64" file="theme_canilike/static/demo/smart-2.jpg"/>
        </record>
        <record id="demo_product_5_image_3" model="product.image">
            <field name="name">Smart watch - image #3</field>
            <field name="product_tmpl_id" ref="demo_product_5"/>
            <field name="image" type="base64" file="theme_canilike/static/demo/smart-3.jpg"/>
        </record>

    </data>
</odoo>