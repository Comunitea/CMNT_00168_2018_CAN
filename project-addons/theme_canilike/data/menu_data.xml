<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <!--Main menu-->
        <record id="can_main_menu" model="website.menu">
            <field name="name">Main Menu</field>
            <field name="website_id" ref="website_base_multi_can.canilike_website"/>
            <field name="website_published">False</field>
            <field name="not_portal">False</field>
            <field name="not_public">False</field>
        </record>

        <!--Home Menu-->
        <record id="can_menu_home" model="website.menu">
            <field name="name">Home</field>
            <field name="url">/</field>
            <field name="page_id" ref="theme_canilike.can_home_page"/>
            <field name="parent_id" ref="theme_canilike.can_main_menu"/>
            <field name="sequence" type="int">10</field>
            <field name="website_id" ref="website_base_multi_can.canilike_website"/>
            <field name="website_published">True</field>
            <field name="not_portal">False</field>
            <field name="not_public">False</field>
        </record>

        <!-- ======================================= Shop menu ======================================= -->

        <record id="can_menu_shop" model="website.menu">
            <field name="name">Shop</field>
            <field name="url">/shop</field>
            <field name="parent_id" ref="theme_canilike.can_main_menu"/>
            <field name="sequence" type="int">20</field>
            <field name="website_id" ref="website_base_multi_can.canilike_website"/>
            <field name="website_published">True</field>
            <field name="not_portal">False</field>
            <field name="not_public">False</field>
        </record>

        <!--Blog Menu-->
        <record id="can_menu_blog" model="website.menu">
            <field name="name">Blog</field>
            <field name="url">/blog</field>
            <field name="parent_id" ref="theme_canilike.can_main_menu"/>
