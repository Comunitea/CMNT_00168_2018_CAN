<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Add custom styles -->
    <template id="can_website_assets_frontend" inherit_id="website_multi_theme.auto_assets_website_can">

        <xpath expr="link[last()]" position="after">
            <t t-call="theme_canilike.custom_css_snippet"/>
        </xpath>

        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_canilike/static/js/main.js"/>
            <script type="text/javascript" src="/theme_canilike/static/js/comunitea.js"/>
        </xpath>

    </template>

</odoo>