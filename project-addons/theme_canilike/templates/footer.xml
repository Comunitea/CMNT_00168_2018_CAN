<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="can_website_footer_custom_template" name="Footer Custom Canilike Template" inherit_id="website_multi_theme.auto_view_can_website_footer_default">
        <xpath expr="//div[@id='footer']//div[hasclass('row')]" position="replace">
            <div>
                <div class="mb4">
                    <a href="https://goo.gl/maps/tGrwNojqzwPpEvin6" target="_blank" alt="Open in Google Maps" title="Open in Google Maps" rel="nofollow">
                        <t t-esc="res_company.street"/><br/>
                        <t t-esc="res_company.zip"/>
                        <t t-esc="res_company.city"/>
                    </a>
                </div>
                <div class="mb4">
                    <a t-field="res_company.email" t-att-href="'mailto:%s' %res_company.email"/>
                </div>
            </div>
        </xpath>
    </template>

</odoo>