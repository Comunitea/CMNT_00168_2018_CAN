<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="website_crm_contactus_form" name="Contact Form Canilike" inherit_id="website_multi_theme.auto_view_can_website_crm_contactus_form">
        <!-- Google recaptcha and Accept Legal contact form -->
        <xpath expr="//div[hasclass('form-field')][last()]" position="after">
            <t t-call="website_base_multi_can .website_crm_contactus_form_recaptcha_snippet"/>
            <t t-call="website_base_multi_can .website_crm_contactus_form_legal_snippet"/>
        </xpath>
    </template>
</odoo>