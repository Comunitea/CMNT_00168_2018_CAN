	<template id="can_portal_my_details_content_template" name="ccount Details Canilike Theme Template Content" inherit_id="theme_canilike.can_portal_my_details_template">
		<xpath expr="//div[@id='wrap']" position="inside">
			<t t-call="portal.portal_my_details"/>
		</xpath>
	</template>

