<?xml version="1.0" encoding="utf-8" ?>

<odoo noupdate="1">

	<!-- ======================================= Main Template ======================================= -->
	<template id="can_main_template" name="Main Canilike Theme Template" inherit_id="website_multi_theme.auto_layout_website_can">

		<xpath expr="//div[@id='wrapwrap']" position="attributes">
			<attribute name="t-if">True == False</attribute>
		</xpath>

		<xpath expr="//div[@id='wrapwrap']" position="after">
			<div class="page_page_page">
				<header class="header">
					<t t-call="theme_canilike.custom_header_snippet"/>
					<!--<t t-call="breadcrumbs_base.bcb_main_wrap"/>-->
				</header>

				<main>
					<t t-raw="0"/>
				</main>

				<footer>
					<!-- <t t-call="theme_canilike.can_website_footer_custom_template"/>-->
					<t t-call="theme_canilike.custom_footer_snippet"/>
				</footer>
			</div>
		</xpath>

	</template>

	<!-- ======================================= Home Template ======================================= -->
	<template id="can_homepage_template" name="Home Canilike Theme Template">
		<t t-call="theme_canilike.can_main_template">
			<div id="wrap"/>
		</t>
	</template>

	<!-- ======================================= Portal part ======================================= -->
	<template id="can_portal_my_home_template" name="My Portal Canilike Theme Template">
		<t t-call="theme_canilike.can_main_template">
			<div id="wrap"/>
		</t>
	</template>
	<template id="can_portal_my_home_content_template" name="My Portal Canilike Theme Template Content" inherit_id="theme_canilike.can_portal_my_home_template">
		<xpath expr="//div[@id='wrap']" position="inside">
			<t t-call="portal.portal_my_home"/>
		</xpath>
	</template>

	<template id="can_portal_my_details_template" name="Account Details Canilike Theme Template">
		<t t-call="theme_canilike.can_main_template">
