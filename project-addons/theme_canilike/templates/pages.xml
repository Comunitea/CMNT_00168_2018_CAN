	<template id="can_newsletter_content_template" name="Newsletter Canilike Theme Template Content" inherit_id="theme_canilike.can_newsletter_template">
		<xpath expr="//div[@id='wrap']" position="inside">


			<t t-call="theme_canilike.can_page_newsletter_template"/>
			<hr/>
			<t t-set="latest_posts" t-value="website.latest_posts(3, 3, 'Our latest news', 'Read all our news')"/>
			<hr/>
			<t t-call="website_blog_base.wbb_latest_post_wrap"/>
			<hr/>
			<t t-call="website_blog_base.wbb_latest_post"/>


		</xpath>

	</template>

