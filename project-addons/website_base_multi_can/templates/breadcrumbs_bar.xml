<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <!-- Remove the My Account breadcrumb's bar -->
    <template id="can_breadcrumbs_account_remove" name="Remove Portal Account BreadCrumbs" inherit_id="portal.portal_layout">
        <xpath expr="//div[hasclass('o_portal')]" position="attributes">
            <attribute name="t-if">True == False</attribute>
        </xpath>
    </template>

</odoo>